.obj-btn{
  $m-unselectable()
  text-align center;
  box-sizing border-box;
  padding 12px 20px;
  outline: none;
  border: none;
  text-decoration: none;
  display: inline-block;
  line-height: normal;
  cursor: pointer;
  font-weight 400;

  &:active, &:visited, &:focus{
    text-decoration: none;
  }
}